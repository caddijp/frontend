{
  "name": "@caddijp/components",
  "version": "0.0.7",
  "description": "CADDi ui components built with React.",
  "license": "MIT",
  "repository": "git@github.com:caddijp/frontend.git",
  "main": "./lib/components.js",
  "module": "./lib/componentsk.modern.js",
  "scripts": {
    "build": "microbundle build -i src/index.ts -o lib/ --jsx=React.createElement --tsconfig tsconfig.build.json",
    "clean": "rimraf lib",
    "prepublishOnly": "yarn build",
    "start-storybook": "start-storybook -c .storybook",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.5.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.0.12",
    "@storybook/addon-storyshots": "^6.0.12",
    "@storybook/react": "^6.0.12",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "jest-styled-components": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "types": "./lib/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "moduleNameMapper": {
      "\\.(scss|css|less|svg)$": "identity-obj-proxy"
    }
  }
}
